@model Rnt.Web.Controllers.ReactModel
@{
    ViewData["Title"] = "React Test Page";

    string cacheKey = Model.modelData.ToString() + (string)Model.stringData;
}

<cache expires-after="@TimeSpan.FromMinutes(1)">
    @Html.React("Components.Hello", new { initialData = Model, compiler = "TypeScript", framework = "React" })
</cache>

<script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.1.0/react.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.1.0/react-dom.js"></script>
<script src="~/components.js"></script>

